        <property name="icon_name">vc-commit-24</property>
        <signal name="activate" handler="on_button_commit_clicked" swapped="no"/>
      </object>
      <accelerator key="M" modifiers="GDK_CONTROL_MASK"/>
    </child>
    <child>
      <object class="GtkAction" id="VcUpdate">
