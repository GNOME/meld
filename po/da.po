msgid "Modified"
msgstr "Ændret"

#: ../data/ui/dirdiff.ui.h:21
msgid "Show modified"
msgstr "Vis ændret"

#: ../data/ui/dirdiff.ui.h:22
msgid "Filters"
msgstr "Filtre"

#: ../data/ui/dirdiff.ui.h:23
